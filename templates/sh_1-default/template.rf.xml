<html xml:lang="fr">
    <head>
        <RENDER_VALUE what="head>content"/>
    </head>
    <RENDER_TAG type="body" what="body>data">
        <div id="mainContainer">
            <RENDER_VALUE what="body>beginning"/>
            <div id="bg_middle">
                <div id="hb_top" style="text-align:center;">
                    <h1 class="topTitle">
                        <RENDER_VALUE what="body>headline"/>
                    </h1>
                </div>
                <div id="fg_top">
                    <div id="myMenu">
                        <RENDER_VALUE what="body>menu_1"/>
                    </div>
                    <div id="content_middle">
                        <RENDER_IFSET what="body>title">
                            <h1><RENDER_VALUE what="body>title"/></h1>
                        </RENDER_IFSET>
                        <RENDER_IFSET what="modules">
                            <div id="modulesBar">
                                <RENDER_LOOP what="modules">
                                    <div class="oneModule">
                                        <RENDER_VALUE what="modules>content"/>
                                    </div>
                                </RENDER_LOOP>
                            </div>
                        </RENDER_IFSET>
                        <RENDER_VALUE what="body>content"/>
                    </div>
                    <div id="bottomband">
                        <div id="bb_top"></div>
                        <div id="bb_middle"><br /><br />
                        </div>
                    </div>
                </div>
            </div>
            <RENDER_VALUE what="body>adminPanel"/>
            <RENDER_VALUE what="body>otherContents"/>
            <div id="copyrights">
                <RENDER_VALUE what="body>copyrights"/>
            </div>
        </div>
        <RENDER_VALUE what="body>analytics"/>
    </RENDER_TAG>
</html>