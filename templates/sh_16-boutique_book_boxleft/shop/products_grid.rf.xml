<div>
    <NORENDER>navigator_display_mode</NORENDER>
    <div class="icon_display">
        <RENDER_IFSET what="nav_bar>display">
            <a href="{selectors>link}list">
                <img src="/images/builder/icon_display/icon_list.png" alt="aperçu en liste" title="aperçu en liste"/>
            </a>&#160;
        </RENDER_IFSET>
        <RENDER_IFSET what="nav_bar>display">
            <a href="{selectors>link}miniature">
                <img src="/images/builder/icon_display/icon_miniature.png" alt="aperçu en miniatures" title="aperçu en miniatures"/>
            </a>&#160;
        </RENDER_IFSET>
        <RENDER_IFSET what="nav_bar>display">
            <a href="{selectors>link}table">
                <img src="/images/builder/icon_display/icon_table.png" alt="aperçu en tableau" title="aperçu en tableau"/>
            </a>&#160;
        </RENDER_IFSET>
    </div>

    <NORENDER>breadcrumbs</NORENDER>
    <div id="shop_navigator" class="shop_arboText">
        <RENDER_VALUE what="i18n>navigator_title"/>
        <RENDER_LOOP what="nav_levels">
            <span class="shop_same_level">
                <span id="{nav_levels>id}" class="shop_levels" onmouseover="UnTip();TagToTip('shop_small_level_{nav_levels>id}',BORDERCOLOR,'#000000')" onmouseout="UnTip()">
                    <a href="{nav_levels>categoryLink}" class="hideLink"><RENDER_VALUE what="nav_levels>name"/></a>
                    <RENDER_VALUE what="nav_levels>separator"/>
                </span>
                <RENDER_IFSET what="nav_levels>hassisters">
                    <div id="shop_small_level_{nav_levels>id}">
                        <RENDER_LOOP what="sisters">
                            <div>
                                <a href="{sisters>categoryLink}" class="shop_arboLink">
                                    <RENDER_VALUE what="sisters>name"/>
                                </a>
                            </div>
                        </RENDER_LOOP>
                    </div>
                </RENDER_IFSET>
            </span>
        </RENDER_LOOP>
    </div>

    <NORENDER>navigator_grid</NORENDER>
    <div class="marginTop_10px">
            <div class="pictosList">
                <RENDER_LOOP what="pictos">
                    <span>
                        <RENDER_IFSET what="pictos>onePicto_image">
                            <a href="{pictos>onePicto_link}">
                                <img src="{pictos>onePicto_image}" class="onePicto" alt="{pictos>onePicto_desc}" title="{pictos>onePicto_desc}"/>
                            </a>
                        </RENDER_IFSET>
                    </span>
                </RENDER_LOOP>
            </div>
        <div class="navigator_list">
            <div class="navigator_preview">
                <a href="{product>navigator_link}">
                    <img src="{product>image}" alt="navigator_preview"/>
                </a>
            </div>
            <div class="descriptionContainer">
                <div class="nameRef_container">
                    <span class="navigator_name">
                        <RENDER_VALUE what="product>name"/>
                    </span>&#160;
                    <span class="navigator_reference">
                        (<RENDER_VALUE what="product>reference"/>)
                    </span>
                </div>
                <div class="navigator_describe">
                    <span class="navigator_property_name">
                        <RENDER_VALUE what="product>input_description"/>
                    </span>&#160;
                    <span>
                        <RENDER_VALUE what="product>description"/>
                    </span>
                </div>
                <RENDER_IFSET what="constants>showQuantity">
                    <div>
                        <span class="navigator_property_name">
                            <RENDER_VALUE what="product>input_stock"/>
                        </span>&#160;
                        <span class="">
                            <RENDER_VALUE what="product>stock"/>
                        </span>
                    </div>
                </RENDER_IFSET>
            </div>
            <RENDER_IFSET what="product>price">
                <div class="navigator_price">
                    <RENDER_VALUE what="product>price"/>
                </div>
            </RENDER_IFSET>
        </div>
        <RENDER_VALUE what="navigator>content"/>
    </div>

</div>